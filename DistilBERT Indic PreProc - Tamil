v(\"/content/drive/MyDrive/CodalabSarcasmData/sarcasm_tam_dev.csv\")\n","tam_test = pd.read_csv(\"/content/drive/MyDrive/CodalabSarcasmData/sarcasm_tam_test_without_labels.csv\")\n","\n","# Define reverse transliteration function\n","def reverse_transliterate_text(text):\n","    return transliterate(text, sanscript.ITRANS, sanscript.TAMIL)\n","\n","# Apply reverse transliteration\n","tam_train['Text'] = tam_train['Text'].apply(reverse_transliterate_text)\n","tam_val['Text'] = tam_val['Text'].apply(reverse_transliterate_text)\n","tam_test['Text'] = tam_test['Text'].apply(reverse_transliterate_text)\n","\n","print(tam_train['Text'])\n","# Initialize tokenizer\n","tokenizer = DistilBertTokenizer.from_pretrained('distilbert-base-multilingual-cased')\n","\n","# Tokenize texts\n","def tokenize_text(text):\n","    return tokenizer.encode_plus(\n","        text,\n","        max_length=128,\n","        padding='max_length',\n","        truncation=True,\n","        return_tensors='tf'\n","    )\n","\n","tam_train['encodings'] = tam_train['Text'].apply(lambda x: tokenize_text(x))\n","tam_val['encodings'] = tam_val['Text'].apply(lambda x: tokenize_text(x))\n","tam_test['encodings'] = tam_test['Text'].apply(lambda x: tokenize_text(x))\n","\n","print(tam_train['encodings'])\n","# Extract encodings\n","def extract_key_from_encodings(series, key):\n","    return tf.concat([enc[key] for enc in series], axis=0)\n","\n","train_input_ids = extract_key_from_encodings(tam_train['encodings'], 'input_ids')\n","train_attention_mask = extract_key_from_encodings(tam_train['encodings'], 'attention_mask')\n","\n","val_input_ids = extract_key_from_encodings(tam_val['encodings'], 'input_ids')\n","val_attention_mask = extract_key_from_encodings(tam_val['encodings'], 'attention_mask')\n","\n","test_input_ids = extract_key_from_encodings(tam_test['encodings'], 'input_ids')\n","test_attention_mask = extract_key_from_encodings(tam_test['encodings'], 'attention_mask')\n","\n","# Map labels\n","label_mapping = {'Sarcastic': 1, 'Non-sarcastic': 0}\n","tam_train['labels'] = tam_train['labels'].map(label_mapping)\n","tam_val['labels'] = tam_val['labels'].map(label_mapping)\n","\n","train_labels = tf.convert_to_tensor(tam_train['labels'].values)\n","val_labels = tf.convert_to_tensor(tam_val['labels'].values)\n","print(tam_train['labels'])\n","# Define custom macro F1 score metric\n","def macro_f1_score(y_true, y_pred):\n","    y_pred = tf.round(y_pred)\n","    tp = tf.reduce_sum(tf.cast(y_true * y_pred, 'float'), axis=0)\n","    tn = tf.reduce_sum(tf.cast((1 - y_true) * (1 - y_pred), 'float'), axis=0)\n","    fp = tf.reduce_sum(tf.cast((1 - y_true) * y_pred, 'float'), axis=0)\n","    fn = tf.reduce_sum(tf.cast(y_true * (1 - y_pred), 'float'), axis=0)\n","\n","    p = tp / (tp + fp + tf.keras.backend.epsilon())\n","    r = tp / (tp + fn + tf.keras.backend.epsilon())\n","\n","    f1 = 2 * p * r / (p + r + tf.keras.backend.epsilon())\n","    macro_f1 = tf.reduce_mean(f1)\n","    return macro_f1\n","\n","# Load pre-trained DistilBERT model\n","model = TFDistilBertModel.from_pretrained('distilbert-base-multilingual-cased')\n","\n","# Define model architecture\n","input_ids_layer = tf.keras.layers.Input(shape=(128,), dtype=tf.int32, name='input_ids')\n","attention_mask_layer = tf.keras.layers.Input(shape=(128,), dtype=tf.int32, name='attention_mask')\n","distilbert_outputs = model(input_ids_layer, attention_mask=attention_mask_layer)\n","clf_output = distilbert_outputs.last_hidden_state[:, 0, :]  # Extract the [CLS] token's output\n","clf_output = tf.keras.layers.Dense(128, activation='relu')(clf_output)\n","clf_output = tf.keras.layers.Dropout(0.5)(clf_output)\n","clf_output = tf.keras.layers.Dense(1, activation='sigmoid')(clf_output)\n","\n","sarcasm_model = tf.keras.Model(inputs=[input_ids_layer, attention_mask_layer], outputs=clf_output)\n","sarcasm_model.compile(optimizer=tf.keras.optimizers.Adam(learning_rate=2e-5), loss='binary_crossentropy', metrics=[macro_f1_score])\n","\n","# Prepare datasets\n","train_dataset = tf.data.Dataset.from_tensor_slices(({\n","    'input_ids': train_input_ids,\n","    'attention_mask': train_attention_mask\n","}, train_labels)).shuffle(1000).batch(32)\n","\n","val_dataset = tf.data.Dataset.from_tensor_slices(({\n","    'input_ids': val_input_ids,\n","    'attention_mask': val_attention_mask\n","}, val_labels)).batch(32)\n","\n","# Train model\n","history = sarcasm_model.fit(train_dataset, epochs=2, validation_data=val_dataset)\n","\n","# Print the model summary\n","sarcasm_model.summary()\n","print(tam_train.head(10))\n"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"INl04Q29tDAA"},"outputs":[],"source":["# Import libraries\n","import pandas as pd\n","import numpy as np\n","import tensorflow as tf\n","from indic_transliteration import sanscript\n","from indic_transliteration.sanscript import transliterate\n","from transformers import DistilBertTokenizer, TFDistilBertModel\n","import nltk\n","\n","# Download NLTK data\n","nltk.download('stopwords')\n","nltk.download('punkt')\n","\n","# Load datasets\n","tam_train = pd.read_csv(\"/content/drive/MyDrive/sarcasm_tam_train.csv\")\n","tam_val = pd.read_csv(\"/content/drive/MyDrive/sarcasm_tam_dev.csv\")\n","tam_test = pd.read_csv(\"/content/drive/MyDrive/sarcasm_tam_test_without_labels_tamil.csv\")"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"U11yO7mWtKYH"},"outputs":[],"source":["# Define reverse transliteration function\n","def reverse_transliterate_text(text):\n","    return transliterate(text, sanscript.ITRANS, sanscript.TAMIL)\n","\n","# Apply reverse transliteration\n","tam_train['Text'] = tam_train['Text'].apply(reverse_transliterate_text)\n","tam_val['Text'] = tam_val['Text'].apply(reverse_transliterate_text)\n","tam_test['Text'] = tam_test['Text'].apply(reverse_transliterate_text)"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"8DzafT3WtX4H"},"outputs":[],"source":["# Initialize tokenizer\n","to