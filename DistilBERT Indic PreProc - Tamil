urn macro_f1"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"AjIolFkdtq1x"},"outputs":[],"source":["# Load pre-trained DistilBERT model\n","model = TFDistilBertModel.from_pretrained('distilbert-base-multilingual-cased')\n","\n","# Define model architecture\n","input_ids_layer = tf.keras.layers.Input(shape=(128,), dtype=tf.int32, name='input_ids')\n","attention_mask_layer = tf.keras.layers.Input(shape=(128,), dtype=tf.int32, name='attention_mask')\n","distilbert_outputs = model(input_ids_layer, attention_mask=attention_mask_layer)\n","clf_output = distilbert_outputs.last_hidden_state[:, 0, :]  # Extract the [CLS] token's output\n","\n","# Reduced the number of units in dense layer and lowered dropout rate\n","clf_output = tf.keras.layers.Dense(64, activation='relu')(clf_output)\n","clf_output = tf.keras.layers.Dropout(0.3)(clf_output)\n","clf_output = tf.keras.layers.Dense(1, activation='sigmoid')(clf_output)\n","\n","sarcasm_model = tf.keras.Model(inputs=[input_ids_layer, attention_mask_layer], outputs=clf_output)\n","sarcasm_model.compile(optimizer=tf.keras.optimizers.Adam(learning_rate=1e-5), loss='binary_crossentropy', metrics=[macro_f1_score])\n","\n","# Print the model summary\n","sarcasm_model.summary()\n"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"XN876wDktq85"},"outputs":[],"source":["# Prepare datasets\n","train_dataset = tf.data.Dataset.from_tensor_slices(({\n","    'input_ids': train_input_ids,\n","    'attention_mask': train_attention_mask\n","}, train_labels)).shuffle(1000).batch(32)\n","\n","val_dataset = tf.data.Dataset.from_tensor_slices(({\n","    'input_ids': val_input_ids,\n","    'attention_mask': val_attention_mask\n","}, val_labels)).batch(32)\n","\n","# Train model\n","history = sarcasm_model.fit(train_dataset, epochs=2, validation_data=val_dataset)\n","\n","# Print the model summary\n","sarcasm_model.summary()\n"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"jbupepu4uSsB"},"outputs":[],"source":["import pandas as pd\n","import tensorflow as tf\n","from indic_transliteration import sanscript\n","from indic_transliteration.sanscript import transliterate\n","from transformers import DistilBertTokenizer, TFDistilBertModel\n","\n","# Define reverse transliteration function\n","def reverse_transliterate_text(text):\n","    return transliterate(text, sanscript.ITRANS, sanscript.TAMIL)\n","\n","# Reverse transliterate the Text column in tam_test\n","tam_test['Text'] = tam_test['Text'].apply(reverse_transliterate_text)\n","\n","# Initialize tokenizer\n","tokenizer = DistilBertTokenizer.from_pretrained('distilbert-base-multilingual-cased')\n","\n","# Tokenize texts\n","def tokenize_text(text):\n","    return tokenizer.encode_plus(\n","        text,\n","        max_length=128,\n","        padding='max_length',\n","        truncation=True,\n","        return_tensors='tf'\n","    )\n","\n","tam_test['encodings'] = tam_test['Text'].apply(lambda x: tokenize_text(x))\n","\n","# Extract encodings\n","def extract_key_from_encodings(series, key):\n","    return tf.concat([enc[key] for enc in series], axis=0)\n","\n","test_input_ids = extract_key_from_encodings(tam_test['encodings'], 'input_ids')\n","test_attention_mask = extract_key_from_encodings(tam_test['encodings'], 'attention_mask')\n","\n","# Define a simplified version of the model (as in the previous response)\n","input_ids_layer = tf.keras.layers.Input(shape=(128,), dtype=tf.int32, name='input_ids')\n","attention_mask_layer = tf.keras.layers.Input(shape=(128,), dtype=tf.int32, name='attention_mask')\n","distilbert_outputs = model(input_ids_layer, attention_mask=attention_mask_layer)\n","clf_output = distilbert_outputs.last_hidden_state[:, 0, :]  # Extract the [CLS] token's output\n","clf_output = tf.keras.layers.Dense(64, activation='relu')(clf_output)\n","clf_output = tf.keras.layers.Dropout(0.3)(clf_output)\n","clf_output = tf.keras.layers.Dense(1, activation='sigmoid')(clf_output)\n","\n","sarcasm_model = tf.keras.Model(inputs=[input_ids_layer, attention_mask_layer], outputs=clf_output)\n","sarcasm_model.compile(optimizer=tf.keras.optimizers.Adam(learning_rate=1e-5), loss='binary_crossentropy', metrics=[macro_f1_score])\n","\n","# Load pre-trained weights (assuming the model has been trained and saved previously)\n","# sarcasm_model.load_weights('path_to_saved_model_weights')\n","\n","# Predict labels for tam_test\n","predictions = sarcasm_model.predict({'input_ids': test_input_ids, 'attention_mask': test_attention_mask})\n","predicted_labels = tf.round(predictions).numpy().astype(int).flatten()\n","\n","# Create a DataFrame with ID and labels\n","results_df = pd.DataFrame({'ID': tam_test['ID'], 'labels': predicted_labels})\n","\n","# Save the DataFrame as a CSV file\n","results_df.to_csv('tam*test*predictions.csv', index=False)\n"]},{"cell_type":"markdown","metadata":{"id":"GuJwmPdvHq84"},"source":[]}],"metadata":{"colab":{"provenance":[{"file_id":"1zFvspdNxGkevUVkfKZK0ngkpdHpteSIe","timestamp":1725778953128}]},"kernelspec":{"display_name":"Python 3","name":"python3"},"language_info":{"name":"python"},"widgets":{"application/vnd.jupyter.widget-state+json":{"01edf0e598924eba999050742cce94a7":{"model_module":"@jupyter-widgets/controls","model_module_version":"1.5.0","model_name":"HTMLModel","state":{"_dom_classes":[],"_model_module":"@jupyter-widgets/controls","_model_module_version":"1.5.0","_model_name":"HTMLModel","_view_count":null,"_view_module":"@jupyter-widgets/controls","_view_module_version":"1.5.0","_view_name":"HTMLView","description":"","description_tooltip":null,"layout":"IPY_MODEL_ebc39c8b12944abeabdef03216e10259","placeholder":"​","style":"IPY_MODEL_e2b2db45d7cc405ba89f537fa2bf72a6","value":" 466/466 [00:00&lt;00:00, 13.2kB/s]"}},"0604fda7d8a24ef88517d4cd1289bfe8":{"model_module":"@jupyter-widgets/controls","model_module_version":"1.5.0","model_name":"FloatProgressModel","state":{"_dom_classes":[],"_model_module":"@jupyter-widgets/controls","_model_module